{
  "extends": "./tsconfig.paths.json",
  "compilerOptions": {
    "lib": [
      "ESNext"
    ],
    "target": "ES2020",
  },
  "include": [
    "src/**/*.ts",
    "serverless.ts"
  ],
  "exclude": [
    "node_modules/**/*",
    ".serverless/**/*",
    ".webpack/**/*",
    "_warmup/**/*",
    ".vscode/**/*"
  ],
  "ts-node": {
    "require": [
      "tsconfig-paths/register"
    ]
  }
}
